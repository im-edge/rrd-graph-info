{
    "name": "imedge/rrd-graph-info",
    "description": "RRD GraphInfo library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\RrdGraphInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\RrdGraphInfo\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "imedge/json": ">=0.1",
        "imedge/rrd-structure": ">=0.2.1",
        "psr/log": "^1.0 || ^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5 || ^10.3",
        "squizlabs/php_codesniffer": "^3.7",
        "symplify/easy-coding-standard": "^12.0.8"
    }
}
